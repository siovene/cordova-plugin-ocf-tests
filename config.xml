<?xml version='1.0' encoding='utf-8'?>
<widget id="com.intel.otc.ocf.tests" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova OCF Plugin Tests</name>
    <description>
        Unit tests for the Cordova OCF Plugin
    </description>
    <author email="salvatore.iovene@intel.com">
        Salvatore Iovene
    </author>
    <content src="cdvtests/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="21" />
        <preference name="android-targetSdkVersion" value="22" />
    </platform>
</widget>
